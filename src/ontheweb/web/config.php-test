function tabsort_commandline($infn, $outfn, $column, $descending) {
	global $tabsort; 
    $cmd = $tabsort;
    if ($descending) {
        $cmd .= " -d";
    }
    $cmd .= " " . $column . " " . $infn . " " . $outfn;
    return $cmd;
}

