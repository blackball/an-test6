#!/bin/bash
FILETYPE=[code here]
curl --silent \\
  --request POST \\
  --data-binary "@$1" \\
  --header "Slug: Sweeping the rock" \\
  --header "Content-Type: image/$FILETYPE" \\
  --header "Authorization: GoogleLogin aupth=$PICASAWEB_Auth" \\
  "http://picasaweb.google.com/data/feed/api/user/$PICASAWEB_USER/albumid/5113621341847124417" \\
  | tidy -xml -indent -quiet
