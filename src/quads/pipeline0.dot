graph indexpipeline {
	graph [ rankdir = LR ];
	node [ shape=box ];
	edge [ dir=forward ];
	node [ style=filled, fillcolor=azure2 ];
	ancat [ label="Astrometry.net Catalog\n(an*.fits)" ];
	id    [ label="Star ID\n(.id)" ];
	skdt  [ label="Star kdtree\n(.skdt)" ];
	node [ style="" ];
	objs  [ label="Catalog\n(.objs)" ];

	node [ shape=ellipse ];
	cut_an [ label="cut-an" ];
	ancat -- cut_an
	cut_an -- objs
	cut_an -- id
	objs -- startree -- skdt
}


