  Executable     = /home/dstn/RUNS/36/launch
  Universe       = vanilla

  # we only want to run on CPU 1: this is an attempt to get only
  # one job scheduled per machine.
  Requirements = (VirtualMachineId == 2)

  Arguments = $(Process)
  Error   = err/err.$(Process)
  Output  = out/out.$(Process)
  Log     = condor.log

  Queue 420

