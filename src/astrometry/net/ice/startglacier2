#! /bin/bash

# called via ssh from gmaps@oven.

. /data1/dstn/dsolver/astrometry/net/ice/ice-config-hydra

killall glacier2router >> ${LOG}/glacier2.log 2>&1

./SessionServer.py >> ${LOG}/sessionserver.log 2>&1 &
sleep 1
${ICEBIN}/glacier2router --Ice.Config=config.glacier2 >> ${LOG}/glacier2.log 2>&1 &

wait

